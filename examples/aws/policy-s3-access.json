{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AdminDumperS3",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": "arn:aws:s3:::YOUR_S3_BUCKET/*"
        },
        {
            "Sid": "ListBuckets",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::*"
        }
    ]
}